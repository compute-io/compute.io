  "bugs": {
    "url": "https://github.com/compute-io/compute.io/issues"
  },
  "dependencies": {
    "compute-csum": "^1.0.0",
    "compute-hamming": "^1.0.0",
    "compute-hypot": "^1.0.0",
    "compute-iqr": "^1.0.0",
    "compute-max": "^1.0.0",
    "compute-mean": "^1.0.0",
    "compute-median": "^1.0.0",
    "compute-min": "^1.0.0",
    "compute-mode": "^1.0.0",
    "compute-quantiles": "^1.0.0",
    "compute-roundn": "^1.0.0",
    "compute-skewness": "^1.0.0",
    "compute-stdev": "^1.0.0",
    "compute-sum": "^1.0.0",
    "compute-variance": "^1.0.0",
    "compute-wmean": "^1.0.0"
